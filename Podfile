# Uncomment the next line to define a global platform for your project
platform :ios, ’10.0’

target 'Viveit' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!


    
    
pod 'Spring', :git => 'https://github.com/MengTo/Spring.git', :branch => 'swift3'

pod ‘Google/SignIn’
pod 'GoogleMaps'
pod 'Firebase/Core'


pod ‘FBSDKCoreKit’
pod ‘FBSDKShareKit’
pod ‘FBSDKLoginKit’

pod ‘Fabric’
pod ‘TwitterKit’
pod ‘TwitterCore’




  # Pods for Viveit

  target 'ViveitTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'ViveitUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
